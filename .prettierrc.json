{
	"printWidth": 100,
	"tabWidth": 3,
	"useTabs": true,
	"semi": false,
	"jsxSingleQuote": true,
	"quotes": false,
	"quoteProps": "consistent",
	"singleQuote": true,
	"trailingComma": "es5",
	"bracketSpacing": false,
	"bracketSameLine": false,
	"arrowParens": "always",
	"proseWrap": "always",
	"singleAttributePerLine": true,
	"parser": "babel-ts",
	"requirePragma": true
}
